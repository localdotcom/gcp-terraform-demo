{
    "terraform": {
        "resources": [
            "gcs",
            "iam",
            "lb"
        ]
    },
    "gcs": {
        "buckets": [
            "one",
            "two"
        ],
        "location": "EU",
        "storage_class": "STANDARD",
        "public_access_prevention": "enforced",
        "uniform_bucket_level_access": true,
        "versioning": {
            "enabled": true
        }
    },
    "iam": {
        "service_account": {
            "name": "sa-private-gcs",
            "display_name": "Service account to access the private GCS bucket"
        }
    },
    "lb": {
        "domain": "site"
    }
}
